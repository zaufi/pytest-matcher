[codespell]
builtin = clear,rare,names,code,en-GB_to_en-US
check-filenames =
context = 2
enable-colors = true
quiet-level = 3
# TODO Add _false positives_ to the ignore list below,
# or use the `# codespell:ignore [word]` comment in code.
# ignore-words-list =
skip = .extras,.password-store,build
